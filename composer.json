{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
      "framework",
      "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "adrianorosa/laravel-geolocation": "^1.1",
        "alchemyguy/youtube-laravel-api": "^1.0",
        "anhskohbo/no-captcha": "^3.5",
        "bacon/bacon-qr-code": "^2.0",
        "graham-campbell/markdown": "14.0",
        "guzzlehttp/guzzle": "^7.5",
        "inertiajs/inertia-laravel": "^0.6.3",
        "ixudra/curl": "^6.22",
        "jeremykenedy/laravel-roles": "^9.1",
        "laravel/fortify": "^1.16",
        "laravel/framework": "^9.19",
        "laravel/passport": "^11.8",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.6",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.2",
        "league/oauth2-client": "^2.6",
        "pestphp/pest": "^1.16",
        "pestphp/pest-plugin-laravel": "^1.1",
        "pragmarx/google2fa-laravel": "^2.0",
        "psecio/oauth2-discord": "dev-master",
        "romanzipp/laravel-twitch": "^4.5",
        "socialiteproviders/discord": "^4.1",
        "socialiteproviders/github": "^4.1",
        "textalk/websocket": "^1.6",
        "tightenco/ziggy": "^1.0",
        "wohali/oauth2-discord-new": "^1.2"
    },
    "require-dev": {
      "fakerphp/faker": "^1.9.1",
      "laravel/breeze": "^1.18",
      "laravel/pint": "^1.0",
      "laravel/sail": "^1.0.1",
      "mockery/mockery": "^1.4.4",
      "nunomaduro/collision": "^6.1",
      "phpunit/phpunit": "^9.5.10",
      "spatie/laravel-ignition": "^1.0"
    },
    "autoload": {
      "files": [
        "app/helpers.php"
      ],
      "psr-4": {
        "App\\": "app/",
        "Database\\Factories\\": "database/factories/",
        "Database\\Seeders\\": "database/seeders/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "scripts": {
      "post-autoload-dump": [
        "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
        "@php artisan package:discover --ansi"
      ],
      "post-update-cmd": [
        "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
      ],
      "post-root-package-install": [
        "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
      ],
      "post-create-project-cmd": [
        "@php artisan key:generate --ansi"
      ]
    },
    "extra": {
      "laravel": {
        "dont-discover": [
          
        ]
      }
    },
    "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true,
      "allow-plugins": {
        "pestphp/pest-plugin": true
      }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
  }
